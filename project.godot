config_version=5

[application]

config/name="Poly64"
run/main_scene="res://Console.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Sprites/PolySplash.png"
config/icon="res://Sprites/icon.svg"

[autoload]

Console="*res://Scripts/Console.gd"

[display]

window/size/viewport_width=240
window/size/viewport_height=180
window/size/window_width_override=640
window/size/window_height_override=480
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/image="res://PolySplash.png"
window/bg_color=Color(0, 0, 0, 1)

[filesystem]

import/fbx/enabled=false

[gui]

theme/custom_font="res://Fonts/acorn-mode-1.ttf"
theme/default_font_antialiasing=0
theme/default_font_subpixel_positioning=3

